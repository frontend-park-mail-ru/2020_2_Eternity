<div class="board">
    <a href="">
        <section class="board__card">
            {{#each coverPins}}
                <img src="{{ this.imgSrc }}" alt="">
            {{/each}}
        </section>
        <section class="board__title">
            <h2>{{ name }}</h2>
            <p>{{ date }}</p>
        </section>
    </a>
</div>
